<Project Sdk="Microsoft.NET.Sdk">


  <PropertyGroup>
    <TargetFramework>net462</TargetFramework>
    <AssemblyName>SqlServerSample</AssemblyName>
    <PackageId>SqlServerSample</PackageId>
    <RuntimeIdentifiers>win</RuntimeIdentifiers>
    <WarningLevel>4</WarningLevel>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <OutputType>exe</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <None Include="App.config" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.1" />
    <PackageReference Include="NServiceBus" Version="6.2.0" />
    <ProjectReference Include="..\Shared\Shared.csproj" />
    <Reference Include="NServiceBus.Persistence.Sql">
      <SpecificVersion>False</SpecificVersion>
      <HintPath>..\..\ForIntegration\NServiceBus.Persistence.Sql.dll</HintPath>
    </Reference>
    <Reference Include="System" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

</Project>
